<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.dite_student.android.dite.tools_6_sem">
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mms"
        android:text="press tool design I"/>


    <Button
        android:layout_below="@id/mms"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mt"
        android:text="metal cutting and tool design"/>

    <Button
        android:id="@+id/plc"
        android:layout_below="@id/mt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="mould design i"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/cim"
        android:layout_below="@id/plc"
        android:text="layered manufacturing "/>
    <Button
        android:id="@+id/ae"
        android:layout_below="@id/cim"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="finite element method"/>
    <Button
        android:layout_below="@id/ae"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="total quality management"/>
</RelativeLayout>
